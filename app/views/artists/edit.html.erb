<div align="center" class="container">
  <br>
    <div class="col-md-4 text-center">
    <%= form_with(model: @artist, local: true) do |f| %>
      <%= f.label :name %><br>
      <%= f.text_field :name, :class => "form-control" %><br><br>
      <%= f.label :discogsid %><br>
      <%= f.text_field :discogsid, :class => "form-control" %><br><br>
      <%= f.label :image %><br>
      <%= f.file_field :avatar %><br><br>
      <%= f.submit %><br>
    <% end %>
    </div>
</div>
