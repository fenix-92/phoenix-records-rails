<div align="center" class="container">
  <br>
  <h2>New artist</h2>
  <br>
    <div class="col-md-4 text-center">
    <%= form_for :artist, url: artists_path do |f| %>
      <%= f.label :name %><br>
      <%= f.text_field :name, :class => "form-control" %><br><br>
      <%= f.label :discogsid %><br>
      <%= f.text_field :discogsid, :class => "form-control" %><br><br>
      <%= f.label :artist_image %><br>
      <%= f.file_field :artist_image %><br><br>
      <%= f.submit %><br>
    <% end %>
    </div>
</div>
